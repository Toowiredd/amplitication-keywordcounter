datasource db {
  provider = "postgresql"
  url      = env("DB_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Keyword {
  count     Int?
  createdAt DateTime @default(now())
  id        String   @id @default(cuid())
  name      String?
  updatedAt DateTime @updatedAt
}

model Occurrence {
  category    String?
  count       Int?
  createdAt   DateTime  @default(now())
  description String?
  detectionId String?
  id          String    @id @default(cuid())
  timestamp   DateTime?
  updatedAt   DateTime  @updatedAt
}
